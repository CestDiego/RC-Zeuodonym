{
  "manifest_version": 2,
  "name": "RC Zeudonym",
  "version": "0.2",

  "icons": {
    "128": "icon.png"
  },

  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["https://*.zulip.com/*"],
      "css": ["styles.css"],
      "js": ["content_script.js"]
    }
  ],

  "permissions": [
    "https://*.zulip.com/*"
	],

  "web_accessible_resources": [
    "*.woff2",
    "*.gif"
  ]
}
